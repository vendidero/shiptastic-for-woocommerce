{
  "name": "vendidero/shiptastic-for-woocommerce",
  "description": "Shiptastic for WooCommerce",
  "homepage": "https://github.com/vendidero/shiptastic-for-woocommerce",
  "license": "GPL-3.0-or-later",
  "type": "wordpress-plugin",
  "prefer-stable": true,
  "version": "dev-main",
  "minimum-stability": "dev",
  "require": {
    "automattic/jetpack-autoloader": "^2.0.0",
    "setasign/fpdi": "^2.3",
    "setasign/fpdf": "^1.8.2",
    "dvdoug/boxpacker": "^3.9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "7.5.14",
    "yoast/phpunit-polyfills": "^1.1.0",
    "woocommerce/woocommerce-sniffs": "^0.1.3"
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": true,
    "platform": {
      "php": "7.1"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": [
      "sh ./bin/vendor-clean.sh"
    ],
    "post-update-cmd": [
      "sh ./bin/vendor-clean.sh"
    ],
    "phpcs": [
      "phpcs -s -p"
    ],
    "phpcs-pre-commit": [
      "phpcs -s -p -n"
    ],
    "phpcbf": [
      "phpcbf -p"
    ]
  },
  "autoload": {
    "psr-4": {
      "Vendidero\\Shiptastic\\": "src"
    }
  }
}
